MONGO_INITDB_ROOT_USERNAME=the_username
MONGO_INITDB_ROOT_PASSWORD=the_password
MONGOEXPRESS_LOGIN=dev
MONGOEXPRESS_PASSWORD=dev
MONGO_INITDB_DATABASE=the_database
MONGO_URL="mongodb://localhost:3456/${MONGO_INITDB_DATABASE}"
MONGO_URL2="mongodb://${MONGO_INITDB_ROOT_USERNAME}:${MONGO_INITDB_ROOT_PASSWORD}@localhost:3456/${MONGO_INITDB_DATABASE}"
